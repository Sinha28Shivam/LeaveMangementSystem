<mvc:View controllerName="leavemgmtsyst.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified">
 
    <Page id="IDGenPage1" title="HCLTech | My Leave">
        <content>
            <l:VerticalLayout id="IDGenVerticalLayout1" width="50%">
                <f:SimpleForm id="IDGenSimpleForm1">
                    <unified:Calendar
                        id="calendar"
                        showWeekNumbers="true"
                        select="handleSelect">
                        <unified:selectedDates>
                            <unified:DateRange id="_IDGenDateRange1" 
                            startDate="{path: '/startDate', type: 'sap.ui.model.type.Date'}" 
                            endDate="{path: '/endDate', type: 'sap.ui.model.type.Date'}" />
                        </unified:selectedDates>
                    </unified:Calendar>
                </f:SimpleForm>
                <Button id="IDGenButton1" text="Submit" type="Success" press="onSubmitLeave" />
                <core:Fragment id="idXMLFrag1" fragmentName="leavemgmtsyst.view.DialogFrag" type="XML" />
            </l:VerticalLayout>
        </content>
    </Page>
</mvc:View>