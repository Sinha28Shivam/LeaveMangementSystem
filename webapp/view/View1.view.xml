<mvc:View  controllerName="leavemgmtsyst.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:panel="sap.f.SidePanel"
    xmlns:f2="sap.f"
    xmlns:unified="sap.ui.unified">
 
    <Page id="IDGenPage1">
    <customHeader>
    <Bar id="_IDGenBar1">
    <contentLeft>
    <Button id="_IDGenBut1" icon="My_images/hcltech.svg"/>
    <Text id="text1" text="My Leave"/>
    </contentLeft>

     <contentRight>    
    <Button id="_IDGenBut3" icon="My_images/apple.png" tooltip="appStore"/>
     <Image id="image2" src="My_images/line.png" height="50px" width="10px"/>
    <Button id="_IDGenBut4" icon="My_images/smile.png" />
    <Button id="_IDGenBut5" icon="My_images/questionMark.png" tooltip="Employee Profile"/>
    <Button id="_IDGenBut6" icon="My_images/setting.png" tooltip="settings"/>
        <MenuButton id="_IDGenBut7" icon="My_images/whitebread.png" tooltip="Menu" type="Transparent" >
        <menu>
        <Menu id="_IDGenMenu1"  visible="true">
        <MenuItem id="_IDGenMenuItem1" text="Profile" />
        <MenuItem id="_IDGenMenuItem2" text="Setting" />
        <MenuItem id="_IDGenMenuItem3" text="Logout" />


        </Menu>
        </menu>

        
        
        </MenuButton>
     <Image id="image3" src="My_images/line.png" height="50px" width="10px"/>
     <Button id="_IDGenBut2" icon="My_images/user.png" tooltip="Employee Profile" press="onPress" />
    </contentRight>
    </Bar>
    </customHeader>




        <content>
        <f2:SidePanel id="_IDGenSidePanel1" sidePanelResizable="true" sidePanelPosition="Left">
        

        </f2:SidePanel>
            <l:VerticalLayout id="IDGenVerticalLayout1" class="centerContent">
                <f:SimpleForm id="IDGenSimpleForm1" layout="ResponsiveGridLayout">
                    <unified:Calendar
                        id="calendar"
                        showWeekNumbers="true"
                        select="handleSelect">
                        <unified:selectedDates>
                            <unified:DateRange id="_IDGenDateRange1" 
                            startDate="{path: '/startDate', type: 'sap.ui.model.type.Date'}" 
                            endDate="{path: '/endDate', type: 'sap.ui.model.type.Date'}" />
                        </unified:selectedDates>
                    </unified:Calendar>
                </f:SimpleForm>
                <Button id="IDGenButton1" text="Apply" press="onSubmitLeave" />
               
            </l:VerticalLayout>
        </content>










        <!-- This is a footer part -->
         <footer>
        <Toolbar id="_IDGenToolbar1" height="50px" style="Standard">
        <content>
        <VBox id="_IDGenHBox1" >
        <HBox id="_IDGenHBox2">
        <Text id="text" text="Best viewed on the latest versions of MS Edge, Chrome or Safari." />
       
        </HBox>

        <!-- <Bar id="_IDGenBar2">
        <contentRight>
        <Image id="_IDGenImage678" src="My_images/hcl.svg" />
        </contentRight>
        </Bar> -->
  </VBox>

        </content>

        </Toolbar>


        </footer>
    </Page>
</mvc:View>